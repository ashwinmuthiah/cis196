<div class="container-fluid">
<%= form_for(@admin) do |f| %>
	<div class = "field">
		<p>name</p>
		<%= f.text_field :name %>
	</div>

		<div class = "field">
		<p>email</p>
		<%= f.text_field :email %>
	</div>

		<div class = "field">
		<p>password</p>
		<%= f.text_field :password %>
	</div>

		<div class = "field">
		<p>status</p>
		<%= f.text_field :status %>
	</div>

    	<div class="actions">
      		<%= f.submit %>
    	</div>

<% end %>
</div>