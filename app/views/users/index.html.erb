<table class="table">
	<tr>
		<td>ID</td>
		<td>Name</td>
		<td>Email</td>
	</tr>
	<% @users.each do |user| %>
	<tr>
		<td><%= user.id %></p>
		<td><%= user.name %></td>
		<td><%= user.email %></td>
		<td><%= user.password %></td>
		<td><%= link_to "show user", user_path(user.id) %></td>
		<td><%= link_to "edit user", edit_user_path(user.id) %></td>
		<td><%= link_to "destroy user", user, confirm: "You sure?", method: :delete %></td>
	</tr>
	<%end%>
</table>